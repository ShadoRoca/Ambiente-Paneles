<?xml version="1.0" ?>
<sdf version="1.8">
  <world name="world_demo">
    <physics name="1ms" type="ignored">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
    </physics>
    <plugin
      filename="libignition-gazebo-physics-system.so"
      name="ignition::gazebo::systems::Physics">
    </plugin>
    <plugin
      filename="libignition-gazebo-user-commands-system.so"
      name="ignition::gazebo::systems::UserCommands">
    </plugin>
    <plugin
      filename="libignition-gazebo-scene-broadcaster-system.so"
      name="ignition::gazebo::systems::SceneBroadcaster">
    </plugin>

    <!--light-->
    <light type="directional" name="sun">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>
    
    <model name="Paneles">
      <static>true</static>
        <link name="link">
          <collision name="collision">
            <geometry>
              <mesh>
                <uri>file://panel_solar/meshes/panel_solar1.stl</uri>
                <scale>0.30 0.30 0.30</scale>
              </mesh>
            </geometry>
          </collision>
          <visual name="visual">
            <geometry>
              <mesh>
                <uri>file://panel_solar/meshes/panel_solar1.dae</uri>
                <scale>0.30 0.30 0.30</scale>
              </mesh>
            </geometry>
          </visual>
        </link>
    </model>

    <!--
    <include>
      <uri>
      model://panel_solar
      </uri>
    </include>
    -->
    
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>40 50</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.8 0.8 0.8 1</specular>
          </material>
        </visual>
      </link>
    </model>
    
    <model name="kobuki_standalone" canonical_link='base_link'>
      <pose relative_to='world'>2 0 0.07 0 0 1.57075</pose>  
      <link name="base_link">
        <visual name='visual'>
          <geometry>
            <mesh>
              <uri>
              file://robot/main_body.dae
              </uri>
              <scale>1 1 1</scale>
            </mesh>
          </geometry>
        </visual>
        <collision name="base">
          <geometry>
              <cylinder> 
              <length>0.10938</length> 
              <radius>0.178</radius> 
             </cylinder>
          </geometry>
        </collision>
        <inertial>
          <inertia>
            <ixx>0.019995</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.019995</iyy>
            <iyz>0.0</iyz>
            <izz>0.03675</izz>
          </inertia>
          <mass>2.4</mass>
          <pose>0.01 0 0 0 0 0</pose>
        </inertial>
      </link>

      <link name="wheel_left_link">
        <pose relative_to='__model__'>0 0.105 -0.03 -1.5707963267948966 0 0</pose> 
        <visual name='visual'>
          <geometry>
            <mesh>
              <uri>
                file://robot/wheel.dae
              </uri>
              <scale>1 1 1</scale>
            </mesh>
          </geometry>
        </visual>
        <collision name='collision'>
          <geometry>
            <cylinder> 
              <length>0.0206</length> 
              <radius>0.035</radius> 
             </cylinder>
          </geometry>
        </collision>
        <inertial>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001</iyy>
            <iyz>0.0</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.01</mass>
        </inertial>
      </link>
      <link name="wheel_right_link">
        <pose relative_to='__model__'>0 -0.105 -0.03 -1.5707963267948966 0 0</pose> 
        <visual name='visual'>1
          <geometry>
            <mesh>
              <uri>
                file://robot/wheel.dae
              </uri>
              <scale>1 1 1</scale>
            </mesh>
          </geometry>
        </visual>
        <collision name='collision'>
          <geometry>
          <cylinder> 
            <length>0.0206</length> 
            <radius>0.035</radius> 
           </cylinder>
          </geometry>
        </collision>
        <inertial>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001</iyy>
            <iyz>0.0</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.01</mass>
        </inertial>
      </link>

      <joint name="wheel_left_joint" type="revolute">
        <parent>base_link</parent>
        <child>wheel_left_link</child>
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz expressed_in='__model__'>0 1 0</xyz>
          <limit>
              <lower>-1.79769e+308</lower> 
              <upper>1.79769e+308</upper>   
          </limit>
        </axis>
      </joint>
      <joint name="wheel_right_joint" type="revolute">
        <parent>base_link</parent>
        <child>wheel_right_link</child>
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz expressed_in='__model__'>0 1 0</xyz>
          <limit>
              <lower>-1.79769e+308</lower> 
              <upper>1.79769e+308</upper>   
          </limit>
        </axis>
      </joint>
      <joint name="caster_front_joint" type="fixed">
        <parent>base_link</parent>
        <child>caster_front_link</child>
        <pose>0.115 0.0 0.007 -1.5707963267948966 0 0</pose>
      </joint>
      <link name="caster_front_link">
        <collision name='collision'>
          <geometry>
            <cylinder> 
              <length>0.0176</length> 
              <radius>0.017</radius> 
            </cylinder>
          </geometry>
        </collision>
        <inertial>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001</iyy>
            <iyz>0.0</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.01</mass>
        </inertial>
      </link>
      <joint name="caster_back_joint" type="fixed">
        <parent>base_link</parent>
        <child>caster_back_link</child>
        <pose>-0.135 0.0 0.009 -1.5707963267948966 0 0</pose>
      </joint>
      <link name="caster_back_link">
        <collision name='collision'>
          <geometry>
            <cylinder> 
              <length>0.0176</length> 
              <radius>0.017</radius> 
            </cylinder>
          </geometry>
        </collision>
        <inertial>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001</iyy>
            <iyz>0.0</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.01</mass>
        </inertial>
      </link>

      <!-- Kobuki's sensors -->
      <joint name="gyro_joint" type="fixed">
        <parent>base_link</parent>
        <child>gyro_link</child>
        <pose>0.056 0.062 0.0202 0 0 0</pose>
      </joint>
      <link name="gyro_link">
        <inertial>
          <inertia>
            <ixx>0.0001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000001</iyy>
            <iyz>0.0</iyz>
            <izz>0.0001</izz>
          </inertia>
          <mass>0.001</mass>
        </inertial>
      </link>
      <joint name="cliff_sensor_left_joint" type="fixed">
        <parent>base_link</parent>
        <child>cliff_sensor_left_link</child>
        <pose>0.08734 0.13601 0.0214 0 1.5707963267948966 0</pose>
      </joint>
      <link name="cliff_sensor_left_link">
        <inertial>
          <inertia>
            <ixx>0.0001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0001</iyy>
            <iyz>0.0</iyz>
            <izz>0.0001</izz>
          </inertia>
          <mass>0.0001</mass>
        </inertial>
      </link>
      <joint name="cliff_sensor_right_joint" type="fixed">
        <parent>base_link</parent>
        <child>cliff_sensor_right_link</child>
        <pose>0.085 -0.13601 0.02140 0 1.5707963267948966 0</pose>
      </joint>
      <link name="cliff_sensor_right_link">
        <inertial>
          <inertia>
            <ixx>0.0001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0001</iyy>
            <iyz>0.0</iyz>
            <izz>0.0001</izz>
          </inertia>
          <mass>0.0001</mass>
        </inertial>
      </link>
      <joint name="cliff_sensor_front_joint" type="fixed">
        <parent>base_link</parent>
        <child>cliff_sensor_front_link</child>
        <pose>0.156 0.00 0.0214 0 1.5707963267948966 0</pose>
      </joint>
      <link name="cliff_sensor_front_link">
        <inertial>
          <inertia>
            <ixx>0.0001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0001</iyy>
            <iyz>0.0</iyz>
            <izz>0.0001</izz>
          </inertia>
          <mass>0.0001</mass>
        </inertial>
      </link>

      <plugin filename="libignition-gazebo-diff-drive-system.so" name="ignition::gazebo::systems::DiffDrive">
        <left_joint>wheel_left_joint</left_joint>
        <right_joint>wheel_right_joint</right_joint>
        <wheel_separation>.230</wheel_separation>
        <wheel_radius>0.035</wheel_radius>
        <odom_publish_frequency>100</odom_publish_frequency>
        <topic>commands/velocity</topic>
        <odom_topic>odom</odom_topic>
      </plugin>

      <!-- Moving Forward-->
      <plugin filename="libignition-gazebo-triggered-publisher-system.so"
              name="ignition::gazebo::systems::TriggeredPublisher">
          <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
              <match field="data">16777235</match>
          </input>
          <output type="ignition.msgs.Twist" topic="/commands/velocity">
              linear: {x: 0.5}, angular: {z: 0.0}
          </output>
      </plugin>
      <!-- Moving Backward-->
      <plugin filename="libignition-gazebo-triggered-publisher-system.so"
              name="ignition::gazebo::systems::TriggeredPublisher">
          <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
              <match field="data">16777237</match>
          </input>
          <output type="ignition.msgs.Twist" topic="/commands/velocity">
              linear: {x: -0.5}, angular: {z: 0.0}
          </output>
      </plugin>
      <!-- Rotating right-->
      <plugin filename="libignition-gazebo-triggered-publisher-system.so"
              name="ignition::gazebo::systems::TriggeredPublisher">
          <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
              <match field="data">16777236</match>
          </input>
          <output type="ignition.msgs.Twist" topic="/commands/velocity">
              linear: {x: 0.0}, angular: {z: -0.5}
          </output>
      </plugin>
      <!--Rotating left-->
      <plugin filename="libignition-gazebo-triggered-publisher-system.so"
              name="ignition::gazebo::systems::TriggeredPublisher">
          <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
              <match field="data">16777234</match>
          </input>
          <output type="ignition.msgs.Twist" topic="/commands/velocity">
              linear: {x: 0.0}, angular: {z: 0.5}
          </output>
      </plugin>

    </model>    
  </world>
</sdf>
